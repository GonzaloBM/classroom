      "source": [
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "import pandas as pd\n",
        "\n",
        "base_url = 'https://www.solotodo.cl/'\n",
        "nombres = []\n",
        "precios = []\n",
        "procesadores = []\n",
        "rams = []\n",
        "almacenamientos = []\n",
        "tarjetas = []\n",
        "pesos = []\n",
        "pntuacion_aplic = []\n",
        "puntuacion_gaming = []\n",
        "puntuacion_mov = []\n",
        "urls = []\n",
        "for i in range (1,2):\n",
        "  url = 'https://www.solotodo.cl/notebooks?ordering=offer_price_usd&page={0}&'.format(i)\n",
        "  page = requests.get(url)\n",
        "  soup = BeautifulSoup(page.content, 'html.parser')\n",
        "  data = (soup.find('div').find_all('h3'))\n",
        "  texto = soup.find_all('a')\n",
        "  for i in range (0,12):\n",
        "    dato = data[i].find_all('a')\n",
        "    name = texto[10+i*3].text.strip()\n",
        "    price = texto[12+i*3].text.strip()\n",
        "    price = price.split(' ')\n",
        "    price = price[1].replace('.',\"\")\n",
        "    urls.append(base_url + dato[0]['href'])\n",
        "    nombres.append(name)\n",
        "    precios.append(int(price))\n",
        "for j in range (0,len(urls)):\n",
        "  pc = requests.get(urls[j])\n",
        "  sopa = BeautifulSoup(pc.content, 'html.parser')\n",
        "  datos_pc = (sopa.find('div'))\n",
        "  for i in range (0,8):\n",
        "    dato = datos_pc.find_all('dd')[i].text.strip()\n",
        "    if i == 0: procesadores.append(dato)\n",
        "    if i == 1: rams.append(dato)\n",
        "    if i == 4: almacenamientos.append(dato)\n",
        "    if i == 5: tarjetas.append(dato)\n",
        "    if i == 7: pesos.append(dato)\n",
        "  for i in [1,3,5]:\n",
        "    dato = sopa.find_all('p')[i].text.strip()\n",
        "    dato = dato.split('/')\n",
        "    numero = int(dato[0])\n",
        "    if i == 1: pntuacion_aplic.append(numero)\n",
        "    if i == 3: puntuacion_gaming.append(numero)\n",
        "    if i == 5: puntuacion_mov.append(numero)\n",
        "  # construimos un diccionario con la información de cada fila\n",
        "  #data = {'url': urls, 'precio': precios, 'procesador': procesadores,'ram': rams, 'almacenamiento': almacenamientos, 'tarjeta de video': tarjetas, 'peso': pesos, 'puntuación aplicaciones': pntuacion_aplic, 'puntuación gaming': puntuacion_gaming, 'puntuación movilidad': puntuacion_mov}\n",
        "\n",
        "    # podemos pasar todo estos a un dataframe\n",
        "    #table = pd.DataFrame(data=data, index=nombres)\n",
        "    #table.to_csv('tabla_ejemplo.csv')\n",
        "gaming = []\n",
        "movilidad = []\n",
        "aplicaciones = []\n",
        "for i in range(0,len(nombres)):\n",
        "  movilidad.append(puntuacion_mov[i]/precios[i])\n",
        "  gaming.append(puntuacion_gaming[i]/precios[i])\n",
        "  aplicaciones.append(pntuacion_aplic[i]/precios[i])\n",
        "mov_max = sorted(zip(movilidad, nombres), reverse=True)[0:10])\n",
        "gam_max = sorted(zip(gaming, nombres), reverse=True)[0:10])\n",
        "app_max = sorted(zip(aplicaciones, nombres), reverse=True)[0:10])\n",
        "\n"
